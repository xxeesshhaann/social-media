<h1>Welcome to your dashboard</h1>

<h2>User Profiles</h2>

<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>    
  </tr>
 
  <% @users.each do |user| %>
    <tr>
      <td><%= user.first_name %></td>
      <td><%= user.last_name %></td>
      <td><%= link_to 'Show', user_path(user) %></td>
      
      
    </tr>
  <% end %>
</table>