<h1>Welcome to your dashboard</h1>

<h2>User Profiles</h2>

<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    
    <th colspan="2"></th>
  </tr>
 
  <% @users.each do |user| %>
    <tr>
      <td><%= user.first_name %></td>
      <td><%= user.last_name %></td>
      
      
    </tr>
  <% end %>
</table>